@model IEnumerable<QuanzhenCinema.Models.TICKET>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>索引</title>
</head>
<body>
    <p>
        @Html.ActionLink("创建", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                订单号
            </th>
            <th>
                排片号
            </th>
            <th>
                列号
            </th>
            <th>
                价格
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MYORDER.ORDER_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SCHEDULE.SCHEDULE_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SEAT.COLUMN_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PRICE)
            </td>
            <td>
                @Html.ActionLink("编辑", "Edit", new { id=item.TICKET_ID }) |
                @Html.ActionLink("详情", "Details", new { id=item.TICKET_ID }) |
                @Html.ActionLink("删除", "Delete", new { id=item.TICKET_ID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
