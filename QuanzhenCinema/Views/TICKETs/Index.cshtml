@model IEnumerable<QuanzhenCinema.Models.TICKET>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MYORDER.ORDER_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SCHEDULE.SCHEDULE_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SEAT.COLUMN_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PRICE)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MYORDER.ORDER_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SCHEDULE.SCHEDULE_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SEAT.COLUMN_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PRICE)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.TICKET_ID }) |
                @Html.ActionLink("Details", "Details", new { id=item.TICKET_ID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.TICKET_ID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
